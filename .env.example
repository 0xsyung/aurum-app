# Aurum App Environment Variables
# Copy this file to .env and fill in the values

# ===========================================
# Network Configuration
# ===========================================

# Chain ID (84532 = Base Sepolia Testnet, 8453 = Base Mainnet)
VITE_CHAIN_ID=84532

# RPC URL for the blockchain network
VITE_RPC_URL=https://sepolia.base.org

# ===========================================
# Smart Contract Addresses (Base Sepolia)
# ===========================================

# MarketFactory contract address
VITE_MARKET_FACTORY_ADDRESS=0xaF8ddE93C551ce4f6A21db07508858Fb15E4bbC9

# AurumOracle contract address
VITE_AURUM_ORACLE_ADDRESS=0x4EC0295F0344ac264EB83bd7bDb0069015702297

# AurumToken (AURUM) contract address
VITE_AURUM_TOKEN_ADDRESS=0xdBfa3D8516C49581e2A6cBbD75F02F24c59811c1

# ConditionalTokens contract address
VITE_CONDITIONAL_TOKENS_ADDRESS=0x9A7A037469204604C29a44901b69B0bBB1d45B13

# FeeCollector contract address
VITE_FEE_COLLECTOR_ADDRESS=0xBCC8aC562085E207460B9a0342d62a480DD9caAC

# ===========================================
# Wallet Configuration
# ===========================================

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
VITE_WALLETCONNECT_PROJECT_ID=

# Privy App ID (get from https://dashboard.privy.io)
VITE_PRIVY_APP_ID=

# x402 Payments (optional)
# Max USDC to spend per request (6 decimals)
VITE_X402_MAX_USDC=0.50

# ===========================================
# API Configuration (Optional)
# ===========================================

# Backend API URL (if applicable)
# VITE_API_URL=https://api.aurumlabs.io
